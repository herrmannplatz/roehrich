{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  "baseBranches": [
    "main"
  ],
  "osvVulnerabilityAlerts": true,
  "packageRules": [{
    "matchManagers": ["gomod"],
    "matchDepTypes": ["indirect"],
    "enabled": true
  }]
  "rangeStrategy": "pin",
  "automerge": true,
  "platformAutomerge": true
}
